        <![endif]-->
  </head>

<!-- Body -->
<body>
    <div class="navbar navbar-default">
      <div class="container">
        <div class="navbar-header">
          <%= link_to "CoderSchoolBlog", root_path, class: "navbar-brand" %>
        </div>
        <div>
          <ul>
            <li>
            <%= link_to "About Me", welcome_index_path, class: "navbar-right navbar-text navbar-link" %>
